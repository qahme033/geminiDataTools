//- extends layout
html
  head
    title=title
    link(href='//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css', rel='stylesheet')
    script(src="/public/js/graph.js")
  body
    div.container
      -var bodyJSON = JSON.parse(body);
      - console.log(bodyJSON[0])
      -var foo = 10;
      div.content
        table.table.table-striped
          thead
          tr
            - for (col in bodyJSON[0])
              th= col 
          tbody
            - for (row in bodyJSON)
                  tr
                  - console.log(row)
                  - for (col in bodyJSON[row])
                    td= bodyJSON[row][col]
  div.footer


